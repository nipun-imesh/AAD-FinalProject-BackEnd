package lk.ijse.gdse.springbootproject.dto;


import lombok.AllArgsConstructor;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;

@AllArgsConstructor
@NoArgsConstructor
@Getter
@Setter
public class CustomerDto {

    private Long id;
    private String name;
    private String address;

    CustomerDto(String name, String address) {

        this.name = name;
        this.address = address;
    }
}
